{
  "compilerOptions": {
    "plugins": [
      {
        "name": "tslint-language-service"
      }
    ],

    "jsx": "react",

    "target": "es6",
    "module": "es2015",
    "moduleResolution": "node",

    "allowSyntheticDefaultImports": true,

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
